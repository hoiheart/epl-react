{
  "name": "functions",
  "scripts": {
    "dev": "next src/app -- -p 5000",
    "build": "rimraf dist && next build src/app && tsc --project src/functions",
    "lint": "tslint -c ./app/tslint.json ./app/**/*.tsx",
    "copy": "cpx \"*{package.json,package-lock.json}\" dist/functions -C && cpx \"src/public/*\" dist/public -C",
    "ins": "cd dist/functions && npm i && npm audit fix",
    "serve": "npm run build && firebase serve --only functions",
    "deploy": "firebase deploy --only functions"
  },
  "engines": {
    "node": "10"
  },
  "main": "index.js",
  "dependencies": {
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "es6-promise": "^4.2.6",
    "express": "^4.17.0",
    "firebase-admin": "~7.0.0",
    "firebase-functions": "^2.3.0",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^8.1.0",
    "next-redux-saga": "^4.0.2",
    "next-redux-wrapper": "^3.0.0-alpha.2",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0-rc.1",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2"
  },
  "devDependencies": {
    "@types/next": "^8.0.5",
    "cpx": "^1.5.0",
    "redux-devtools-extension": "^2.13.8",
    "rimraf": "^2.6.3",
    "tslint": "^5.12.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.2.2"
  },
  "private": true
}
